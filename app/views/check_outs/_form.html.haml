= form_for(@check_out) do |f|
  - if @check_out.errors.any?
    #error_explanation
      %h2
        = pluralize(@check_out.errors.count, "error")
        prohibited this check_out from being saved:
      %ul
        - @check_out.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :item_id
    %br/
    = f.text_field :item_id
  .field
    = f.label :user_id
    %br/
    = f.text_field :user_id
  .field
    = f.label :due_date
    %br/
    = f.date_select :due_date
  .actions
    = f.submit
