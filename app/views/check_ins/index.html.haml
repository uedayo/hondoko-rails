%h1 Listing check_ins
%table
  %thead
    %tr
      %th Check out
      %th
      %th
      %th
  %tbody
    - @check_ins.each do |check_in|
      %tr
        %td= check_in.check_out
        %td= link_to 'Show', check_in
        %td= link_to 'Edit', edit_check_in_path(check_in)
        %td= link_to 'Destroy', check_in, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Check in', new_check_in_path
