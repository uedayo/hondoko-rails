%h1 Listing divisions
%table
  %thead
    %tr
      %th Name
      %th Description
      %th
      %th
      %th
  %tbody
    - @divisions.each do |division|
      %tr
        %td= division.name
        %td= division.description
        %td= link_to 'Show', division
        %td= link_to 'Edit', edit_division_path(division)
        %td= link_to 'Destroy', division, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Division', new_division_path
